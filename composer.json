{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^2.0",
        "doctrine/lexer": "^2.1",
        "drupal/add_content_by_bundle": "^1.2",
        "drupal/admin_toolbar": "^3.6",
        "drupal/antibot": "^2.0",
        "drupal/backup_migrate": "^5.1",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/better_messages": "^2.0",
        "drupal/better_social_sharing_buttons": "^4.1",
        "drupal/block_class": "^4.0",
        "drupal/bootstrap5": "^4.0",
        "drupal/bootstrap_layouts": "5.5",
        "drupal/calendar": "^1.0@beta",
        "drupal/calendar_datetime": "1.x-dev@dev",
        "drupal/captcha": "^2.0",
        "drupal/ckeditor": "1.0",
        "drupal/classy": "^2.0",
        "drupal/color": "1.0.3",
        "drupal/colorbox": "^2.1",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core-composer-scaffold": "10.5",
        "drupal/core-project-message": "10.5",
        "drupal/core-recommended": "10.5",
        "drupal/csv_serialization": "^4.0",
        "drupal/devel": "^5.3",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity": "^1.6",
        "drupal/entity_bs_accordion_tab_formatter": "^2.0",
        "drupal/entity_clone": "^2.1@beta",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/feeds": "3.0",
        "drupal/field_group": "^4.0",
        "drupal/file_entity": "^2.1",
        "drupal/footermap": "1.9",
        "drupal/fullcalendar_view": "^5.2",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "2.2.2",
        "drupal/image_widget_crop": "3.0",
        "drupal/imagefield_slideshow": "^2.0",
        "drupal/imagick": "^1.12",
        "drupal/imce": "^3.1",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/layout_builder_modal": "^2.0",
        "drupal/libraries": "4.0",
        "drupal/mailsystem": "^4.5",
        "drupal/maxlength": "^3.1",
        "drupal/mysql57": "^1.0",
        "drupal/node_read_time": "^1.15",
        "drupal/obfuscate_email": "^2.1",
        "drupal/page_manager": "^4.0@RC",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.13",
        "drupal/quickedit": "^1.0",
        "drupal/range": "^1.6",
        "drupal/rdf": "^2.1",
        "drupal/redirect": "^1.11",
        "drupal/rules": "^4.0",
        "drupal/scheduler": "^2.2",
        "drupal/shs": "^2.0",
        "drupal/simple_gmap": "4.0",
        "drupal/sitemap": "^2.0",
        "drupal/smart_date_recur": "^4.2",
        "drupal/smtp": "1.4",
        "drupal/taxonomy_menu": "3.7",
        "drupal/token_filter": "^2.2",
        "drupal/twig_tweak": "^3.4",
        "drupal/views_bootstrap": "^5.4",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_data_export": "^1.6",
        "drupal/views_field_view": "1.0.0-beta6",
        "drupal/views_slideshow": "^5.0",
        "drupal/views_slideshow_cycle": "^5.0",
        "drupal/webform": "^6.2",
        "drupal/webformautosave": "^2.0",
        "drupal/weight": "3.6",
        "drupal/xmlsitemap": "1.5",
        "drush/drush": "13"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
